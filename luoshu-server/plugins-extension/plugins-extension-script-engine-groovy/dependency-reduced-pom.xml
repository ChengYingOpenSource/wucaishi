  <version>1.0.0-SNAPSHOT</version>
  <url>https://github.com/ChengYingOpenSource/wucaishi</url>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-shade-plugin</artifactId>
        <version>3.2.4</version>
